<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ajaxweb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <page-encoding>UTF-8</page-encoding>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/layout01/view/*</url-pattern>
      <include-prelude>/layout01/incs/top.jspf</include-prelude>
      <include-coda>/layout01/incs/bottom.jspf</include-coda>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/layout02/view/*</url-pattern>
      <include-prelude>/layout02/incs/top.jspf</include-prelude>
      <include-coda>/layout02/incs/bottom.jspf</include-coda>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/bootstrap2/view/*</url-pattern>
      <include-prelude>/bootstrap2/incs/top.jspf</include-prelude>
      <include-coda>/bootstrap2/incs/bottom.jspf</include-coda>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/sj_bootstrap/view/*</url-pattern>
      <include-prelude>/sj_bootstrap/incs/top.jspf</include-prelude>
      <include-coda>/sj_bootstrap/incs/bottom.jspf</include-coda>
    </jsp-property-group>
    <jsp-property-group>
      <url-pattern>/sj_bootstrap2/view/*</url-pattern>
      <include-prelude>/sj_bootstrap2/incs/top.jspf</include-prelude>
      <include-coda>/sj_bootstrap2/incs/bottom.jspf</include-coda>
    </jsp-property-group>
  </jsp-config>
</web-app>